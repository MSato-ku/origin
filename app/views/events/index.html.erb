        <div class="event">
            <div class="main-box">
            <div class="left-container"><%= t.party_name %></div>
            <div class="right-container">
                <%= link_to "詳細", event_path(t.id) %>
                <%= link_to "編集", edit_event_path(t.id) %>
                <%# <%= link_to "削除", event_path(t.id), method: :delete %>
                <%= button_to '削除', event_path(t.id), :method => :delete, class:'button' %>
            </div>
            </div>
        </div>
        <% end %>
</div>

